<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.beijing.ocean.multmediademo.activity.SQLiteActivity">



    <TextView
        android:id="@+id/tv_creat"
        style="@style/my_btn_style"
        android:text="数据库删除数据 "

        />


    <TextView
        android:layout_marginTop="@dimen/dimen_10"
        android:id="@+id/tv_insert"
        style="@style/my_btn_style"
        android:text="数据库插入 "

        />


    <TextView
        android:layout_marginTop="@dimen/dimen_10"
        android:id="@+id/tv_update"
        style="@style/my_btn_style"
        android:text="数据库更新 "

        />



    <TextView
        android:layout_marginTop="@dimen/dimen_10"
        android:id="@+id/tv_search"
        style="@style/my_btn_style"
        android:text="数据库打印数据 "

        />

    <com.beijing.ocean.multmediademo.view.FlowLayout
        android:background="@color/bg_exception_black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.ChasingDots"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.WanderingCubes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.ThreeBounce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.Wave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.WanderingCubes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.ThreeBounce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.RotatingPlane"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.Pulse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />

        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.CubeGrid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.DoubleBounce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small.FadingCircle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Pulse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />
        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"

            android:padding="15dp"
            style="@style/SpinKitView.Small"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />

        <com.github.ybq.android.spinkit.SpinKitView

            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:padding="15dp"
            style="@style/SpinKitView.Small.ChasingDots"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:SpinKit_Color="@android:color/white" />

    </com.beijing.ocean.multmediademo.view.FlowLayout>


</LinearLayout>
